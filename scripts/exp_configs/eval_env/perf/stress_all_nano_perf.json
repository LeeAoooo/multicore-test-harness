{
  "coremark": {
    "sut": "../bin/sut/coremark.exe 0x0 0x0 0x66 1000",
    "instrument_cmd": "bash perf_scripts/selected_perf.sh",
    "mapping": {
      "1" : "../tuned_enemies/tuned_bus",
      "2" : "../tuned_enemies/tuned_bus",
      "3" : "../tuned_enemies/tuned_bus",
      "4" : "../tuned_enemies/tuned_cache",
      "5" : "../tuned_enemies/tuned_cache",
      "6" : "../tuned_enemies/tuned_memory",
      "7" : "../tuned_enemies/tuned_memory"
    },
    "cores": 7,
    "quantile": 0.9,
    "measurement_iterations_step": 20,
    "measurement_iterations_max": 200,
    "max_confidence_variation": 5,
    "confidence_interval": 0.95,
    "stopping" : "no_decrease",
    "governor" : "powersave",
    "max_temperature": 80
  }
}
